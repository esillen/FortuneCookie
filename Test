

// delete same string in a list
def getUniqueItems(iterable):
    seen = set()
    result = []
    for item in iterable:
        if item not in seen:
            seen.add(item)
            result.append(item)
    return result

a = ["asd","def","ase","dfg","asd","def","dfg"]
print getUniqueItems(a)

// create new matrix
import numpy as np
d = np.zeros((2,3))
d[1][2] = 1

